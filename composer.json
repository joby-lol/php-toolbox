{
    "name": "joby/toolbox",
    "description": "A lightweight collection of useful general purpose PHP tools with no dependencies. Committed to always at least having minimal dependencies.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Joby\\Toolbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joby\\Toolbox\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Joby Elliot",
            "email": "code@byjoby.com"
        }
    ],
    "require": {
        "php": "~8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan": "^2"
    }
}
